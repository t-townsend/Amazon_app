<hr>
<%= simple_form_for @product do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :image %>
    <%= f.input :category, collection: @category%>
    <%= f.association :tags, input_html: { class: 'chosen-select'} %>
    <%= f.submit class: 'btn btn-primary'%>
<% end %>
